<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cybertek.taptapkittycat" version="1.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Tap Tap Kitty Cat</name>
    <description>
        Help the kitty cat collect the coins, while avoiding the dangerous spikes and the evil cloud!
    </description>
    <author email="support@cybertek.io" href="http://cybertek.io">
        Cybertek Systems LLC
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="portrait" />
    <platform name="android">
        <icon src="res/caticon.png" />
    </platform>
    <plugin name="cordova-plugin-admobpro" spec="~2.28.3" />
    <plugin name="cordova-plugin-device" spec="~1.1.5" />
    <plugin name="com.darktalker.cordova.screenshot" spec="https://github.com/gitawego/cordova-screenshot.git" />
</widget>
